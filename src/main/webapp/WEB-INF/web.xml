<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>SkillServlet</servlet-name>
        <servlet-class>servlets.SkillServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>servlets.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeamServlet</servlet-name>
        <servlet-class>servlets.TeamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjectServlet</servlet-name>
        <servlet-class>servlets.ProjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerServlet</servlet-name>
        <servlet-class>servlets.CustomerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SkillServlet</servlet-name>
        <url-pattern>/skill</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

<servlet-mapping>
    <servlet-name>TeamServlet</servlet-name>
    <url-pattern>/team</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>ProjectServlet</servlet-name>
    <url-pattern>/project</url-pattern>
</servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerServlet</servlet-name>
        <url-pattern>/customer</url-pattern>
    </servlet-mapping>


</web-app>
